<Window x:Class="Letys.Parrot.Views.CorrectAnswerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:fa="http://schemas.fontawesome.io/icons/"
        Title="Prawidłowa odpowiedź"
        Height="163.4"
        Width="499.282"
        WindowStyle="ToolWindow"
        SizeToContent="Height"
        WindowStartupLocation="CenterScreen"
        FontFamily="Verdana">
    <StackPanel  Style="{StaticResource panelStyle}">
        <TextBlock Name="tblAnswer" Style="{StaticResource answerTextBlockStyle}" />
        <TextBlock Name="tblQuestion" Style="{StaticResource questionTextBlockStyle}" />
        <TextBlock Name="tblExample" Style="{StaticResource exampleTextBlockStyle}" />
    </StackPanel>
</Window>
